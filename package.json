{
  "name": "alsham-360-prima",
  "version": "2.2.0",
  "description": "ALSHAM 360° PRIMA — CRM Enterprise com IA, Gamificação e Automações Integradas (Build Estável v6.0.4 - Enterprise Architecture + React Hybrid v2.2.0-SUPREMO)",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "npm run build:css && vite --host 0.0.0.0",
    "dev:test": "vite --host 127.0.0.1 --port 4173",
    "build": "npm run build:css && vite build",
    "build:css": "tailwindcss -i ./css/input.css -o ./css/tailwind.min.css --minify",
    "watch:css": "tailwindcss -i ./css/input.css -o ./css/tailwind.min.css --watch",
    "preview": "vite preview --host 0.0.0.0",
    "server:dev": "nodemon server.js",
    "deploy": "vercel --prod",
    "deploy:local": "node scripts/deploy-local.js",
    "clean": "rm -rf dist node_modules/.vite css/tailwind.min.css",
    "clean:all": "rm -rf dist node_modules package-lock.json css/tailwind.min.css",
    "fresh-install": "npm run clean:all && npm install && npm run build:css",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "test": "npm run test:unit",
    "test:unit": "vitest run",
    "test:e2e": "start-server-and-test dev:test http://127.0.0.1:4173 \"cypress run\"",
    "test:coverage": "vitest run --coverage",
    "audit:fix": "npm audit fix --force"
  },
  "engines": {
    "node": "22.x",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.46.1",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.9",
    "@tailwindcss/typography": "0.5.15",
    "@vitejs/plugin-legacy": "5.4.2",
    "autoprefixer": "10.4.20",
    "canvas-confetti": "1.9.3",
    "chart.js": "4.4.6",
    "clsx": "^2.1.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.21.2",
    "express-rate-limit": "7.4.1",
    "framer-motion": "^11.0.8",
    "helmet": "8.0.0",
    "jspdf": "2.5.2",
    "lucide-react": "^0.533.0",
    "morgan": "1.10.0",
    "papaparse": "5.4.1",
    "postcss": "8.4.47",
    "posthog-js": "^1.282.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "react-chartjs-2": "^5.3.1",
    "redis": "4.7.0",
    "zustand": "^4.4.7",
    "tailwindcss": "3.4.14",
    "vite": "5.4.20",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-pwa": "0.20.5",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "2.1.4",
    "cssnano": "7.0.6",
    "cypress": "15.5.0",
    "eslint": "9.15.0",
    "jsdom": "^27.0.1",
    "nodemon": "3.1.7",
    "rollup-plugin-visualizer": "5.12.0",
    "start-server-and-test": "2.0.3",
    "typescript": "^5.8.3",
    "vitest": "2.1.4",
    "vercel": "^36.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AbnadabyBonaparte/ALSHAM-360-PRIMA.git"
  },
  "bugs": {
    "url": "https://github.com/AbnadabyBonaparte/ALSHAM-360-PRIMA/issues"
  },
  "homepage": "https://app.alshamglobal.com.br",
  "keywords": [
    "crm",
    "enterprise",
    "saas",
    "automation",
    "ai",
    "gamification",
    "alsham",
    "react",
    "supabase",
    "vite",
    "typescript"
  ],
  "author": {
    "name": "Abnadaby Bonaparte",

{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": null,
  "routes": [
    {
      "src": "^/dashboard\\.html$",
      "dest": "/dashboard.html"
    },
    {
      "src": "^/dashboard$",
      "dest": "/dashboard.html"
    },
    {
      "src": "^/leads\\.html$", 
      "dest": "/leads.html"
    },
    {
      "src": "^/leads$",
      "dest": "/leads.html"
    },
    {
      "src": "^/leads-real\\.html$",
      "dest": "/leads-real.html"
    },
    {
      "src": "^/leads-real$",
      "dest": "/leads-real.html"
    },
    {
      "src": "^/login\\.html$",
      "dest": "/login.html"
    },
    {
      "src": "^/login$",
      "dest": "/login.html"
    },
    {
      "src": "^/register\\.html$",
      "dest": "/register.html"
    },
    {
      "src": "^/register$",
      "dest": "/register.html"
    },
    {
      "src": "^/automacoes\\.html$",
      "dest": "/automacoes.html"
    },
    {
      "src": "^/automacoes$",
      "dest": "/automacoes.html"
    },
    {
      "src": "^/gamificacao\\.html$",
      "dest": "/gamificacao.html"
    },
    {
      "src": "^/gamificacao$",
      "dest": "/gamificacao.html"
    },
    {
      "src": "^/relatorios\\.html$",
      "dest": "/relatorios.html"
    },
    {
      "src": "^/relatorios$",
      "dest": "/relatorios.html"
    },
    {
      "src": "^/configuracoes\\.html$",
      "dest": "/configuracoes.html"
    },
    {
      "src": "^/configuracoes$",
      "dest": "/configuracoes.html"
    },
    {
      "src": "^/create-org\\.html$",
      "dest": "/create-org.html"
    },
    {
      "src": "^/create-org$",
      "dest": "/create-org.html"
    },
    {
      "src": "^/test-supabase\\.html$",
      "dest": "/test-supabase.html"
    },
    {
      "src": "^/test-supabase$",
      "dest": "/test-supabase.html"
    },
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/src/(.*)",
      "dest": "/src/$1"
    },
    {
      "src": "^/$",
      "dest": "/index.html"
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Cache-Control", 
          "value": "public, max-age=3600"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}


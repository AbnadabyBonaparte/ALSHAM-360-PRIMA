{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/dashboard.html",
      "dest": "/dashboard.html"
    },
    {
      "src": "/leads.html", 
      "dest": "/leads.html"
    },
    {
      "src": "/leads-real.html",
      "dest": "/leads-real.html"
    },
    {
      "src": "/login.html",
      "dest": "/login.html"
    },
    {
      "src": "/register.html",
      "dest": "/register.html"
    },
    {
      "src": "/relatorios.html",
      "dest": "/relatorios.html"
    },
    {
      "src": "/automacoes.html",
      "dest": "/automacoes.html"
    },
    {
      "src": "/gamificacao.html",
      "dest": "/gamificacao.html"
    },
    {
      "src": "/configuracoes.html",
      "dest": "/configuracoes.html"
    },
    {
      "src": "/create-org.html",
      "dest": "/create-org.html"
    },
    {
      "src": "/test-supabase.html",
      "dest": "/test-supabase.html"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|webp|mp4|webm|ogg|mp3|wav|flac|aac))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
